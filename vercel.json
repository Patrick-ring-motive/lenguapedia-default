{
   "version": 2,
   "name": "lenguapedia",
   "builds": [
      { "src": "index.mjs", "use": "@vercel/node" }
   ],
   "routes": [
      { "src": "/(.*)", "dest": "/index.mjs" }
   ],
  "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=96400, s-max-age=96400, stale-if-error=31535000, stale-while-revalidate=31535000"
        }
      ]
}